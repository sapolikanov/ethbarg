	file = {s0967-067x2900041-2.pdf:C\:\\Users\\Stepan Polikanov\\Zotero\\storage\\YQPF4V6M\\s0967-067x2900041-2.pdf:application/pdf},
}

@article{filippov_asymmetric_1999,
	title = {Asymmetric bilateral bargaining in the new Russian Federation: A path-dependence explanation},
	volume = {32},
	rights = {https://www.elsevier.com/tdm/userlicense/1.0/},
	issn = {0967-067X, 1873-6920},
	url = {https://online.ucpress.edu/cpcs/article/32/1/61/446/Asymmetric-bilateral-bargaining-in-the-new-Russian},
	doi = {10.1016/S0967-067X(98)00022-1},
	shorttitle = {Asymmetric bilateral bargaining in the new Russian Federation},
	abstract = {In this essay we identify economic and political factors that led both the federal centre and the regions in Russia first to open the process of federal bargaining and then to pursue it in the form of signing bilateral treaties, unique for each region. Many Russian politicians and most scholars of Russian politics view asymmetric bilateral bargaining as a dangerous institutional choice contributing to federal instability and potentially threatening the disintegration of Russia. We offer an alternative view. While the treaty-signing practices are actively maintained by Russian political elites, we argue that the genesis of asymmetric bilateral bargaining in Russia had a strong ‘path dependence’ component. In particular, it was precipitated by the developments of the last period in evolution of the Soviet federalism.},
	pages = {61--76},
	number = {1},
	journaltitle = {Communist and Post-Communist Studies},
	author = {Filippov, Mikhail and Shvetsova, Olga},
	urldate = {2024-06-16},
	date = {1999-03-01},
	langid = {english},
	file = {Filippov and Shvetsova - 1999 - Asymmetric bilateral bargaining in the new Russian.pdf:C\:\\Users\\Stepan Polikanov\\Zotero\\storage\\IP7FYIWI\\Filippov and Shvetsova - 1999 - Asymmetric bilateral bargaining in the new Russian.pdf:application/pdf},
}

