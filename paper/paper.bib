	rights = {https://www.elsevier.com/tdm/userlicense/1.0/},
	issn = {0967-067X, 1873-6920},
	url = {https://online.ucpress.edu/cpcs/article/31/4/359/436/Striking-the-Federal-Bargain-in-Russia-Comparative},
	doi = {10.1016/S0967-067X(98)00017-8},
	shorttitle = {Striking the Federal Bargain in Russia},
	abstract = {The central research goals of this article are to classify and explain the positions of the 89 state “governors” of Russia with regard to the most desirable federal division of power. The state governors are classified along a 5-item autonomy index based on events data and content analysis of their speeches, declarations, petitions, threats and actions from 1991–1995 as reported in two regional sources. Theoretical propositions derived from four schools of thought (essentialism, instrumentalism, relative deprivation and resource mobilization) are tested to discover which, if any, provide useful insights into the preferences and behavior of regional elites in Russia. In the end, a combined model that synthesizes elements of the above is shown to be most useful in explaining variation in elite positions.},
	pages = {359--380},
	number = {4},
	journaltitle = {Communist and Post-Communist Studies},
	author = {Dowley, Kathleen M.},
	urldate = {2024-06-12},
